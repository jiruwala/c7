	<queryString>
		<![CDATA[SELECT PUR2.REFER, PUR2.TYPE,INVOICE_NO,o.attn,
ITEMS.DESCR, PUR2.PACKD, sum(PUR2.ALLQTY) allqty, PUR2.PRICE, sum(PUR2.DISC_AMT) DISC_AMT2,PUR2.PACK,
sum((PUR2.PRICE)*PUR2.ALLQTY/PUR2.PACK) AMOUNT,sum(ALLQTY/PUR2.PACK) PKX
FROM pur2, ITEMS,c_order1 o
WHERE PUR2.INVOICE_CODE=21
and pur2.ordwas=o.keyfld and o.ord_pos=orderno
 AND  (ITEMS.REFERENCE=PUR2.REFER)
 and pur2.keyfld=$P{KEYFLD}
 group by pur2.refer,pur2.type,pur2.invoice_no,items.descr,pur2.packd,pur2.price,pur2.pack,o.attn
ORDER BY PUR2.refer]]>
